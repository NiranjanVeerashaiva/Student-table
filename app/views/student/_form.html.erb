<%= form_with model: student do |form| %>
<% if form.object.errors.any? %>
<% form.object.errors.full_messages.each do |message| %>
<div><%= message %></div>
<% end %>
<% end %>
<div>
    <%= form.label :name %>
    <%= form.text_field :name %>
</div>
<div>
    <%= form.label :place %>
    <%= form.text_area :place %>
</div>
<div>
    <%= form.label :publieshed_at %>
   <div class="p-2"> <%= form.datetime_select :publieshed_at, include_blank: true %></div>
</div>
<%= form.button %>
<% end %>